{
  "name": "telegram_leads",
  "version": "1.0.1",
  "description": "telegram leads listener",
  "main": "index.js",
  "scripts": {
    "build": "netlify-lambda build dist",
    "start": "netlify-lambda serve dist",
    "serve": "netlify dev",
    "dev": "nodemon -r dotenv/config src/server_local.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "nodemonConfig": {
    "delay": "2000",
    "ext": "ts",
    "execMap": {
      "ts": "ts-node"
    },
    "ignore": [
      ".git",
      "node_modules/",
      "node_modules/**/node_modules"
    ]
  },
  "author": "revenage",
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^1.6.0",
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "input": "^1.0.1",
    "netlify-lambda": "^2.0.16",
    "node-cache": "^5.1.2",
    "random-bigint": "^0.0.1",
    "serverless-http": "^3.2.0",
    "telegram": "^2.17.10"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "dotenv-cli": "^7.2.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
